<mujoco model="fr5">
    <worldbody>
        <body name="robot">
            <joint name="joint1" type="hinge" axis="0 0 1" />
            <joint name="joint2" type="hinge" axis="0 1 0" />
            <joint name="joint3" type="hinge" axis="1 0 0" />
            <joint name="joint4" type="hinge" axis="0 0 1" />
            <joint name="joint5" type="hinge" axis="0 1 0" />
            <joint name="joint6" type="hinge" axis="1 0 0" />
            <body name="left_arm">
                <geom name="left_hand" type="box" size="0.1 0.1 0.1" pos="0 0 0" />
            </body>
            <body name="right_arm" pos="0.5 0 0">
                <geom name="right_hand" type="box" size="0.1 0.1 0.1" pos="0 0 0" />
            </body>
        </body>
        <body name="board" pos="0 0 -0.1">
            <geom name="board_geom" type="box" size="0.5 0.1 0.02" />
            <body name="slot" pos="0 0.05 0">
                <geom name="slot_geom" type="box" size="0.05 0.02 0.02" />
            </body>
        </body>
        <body name="target" pos="1 0 0">
            <geom name="target_geom" type="box" size="0.1 0.1 0.02" />
        </body>
    </worldbody>
    <actuator>
        <motor joint="joint1" ctrlrange="-1 1" />
        <motor joint="joint2" ctrlrange="-1 1" />
        <motor joint="joint3" ctrlrange="-1 1" />
        <motor joint="joint4" ctrlrange="-1 1" />
        <motor joint="joint5" ctrlrange="-1 1" />
        <motor joint="joint6" ctrlrange="-1 1" />
    </actuator>
</mujoco>
